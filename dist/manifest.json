{
  "name": "Discovery",
  "version": "0.0",
  "description": "Chrome extension testing!",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"]
}
